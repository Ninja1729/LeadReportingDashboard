<coordinator-app name="leadreporting"
                 frequency="${coord:days(1)}"
                 start="${startDate}"
                 end="${endDate}"
                 timezone="UTC"
                 xmlns="uri:oozie:coordinator:0.2">
    <controls>
        <concurrency>1</concurrency>
    </controls>

    <!-- Format of startDate and endDate is: YYYY-MM-DDTMM:SSZ-->
    <datasets>
        <!-- Input datasets-->
        <dataset name="lead-input" frequency="${coord:days(1)}"
                 initial-instance="2014-01-01T00:00Z" timezone="UTC">
            <uri-template>${nameNode}/${leadLocation}/${YEAR}/${MONTH}/${DAY}</uri-template>
            <done-flag></done-flag>
        </dataset>
    </datasets>
    <input-events>
        <data-in name="pricing-input-daily" dataset="lead-input">
            <instance>${coord:current(-1)}</instance>
        </data-in>
    </input-events>

    <action>
        <workflow>
            <app-path>${workflowAppPath}</app-path>
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>




            </configuration>
        </workflow>
    </action>
</coordinator-app>